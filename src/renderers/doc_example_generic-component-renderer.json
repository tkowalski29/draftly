        {
          "name": "[Input] Text Field System",
          "type": "GENERIC",
          "componentConfig": {
            "figmaNodeType": "FRAME",
            "properties": {
              "autoLayout": {
                "direction": "VERTICAL",
                "spacing": "${labelSpacing}"
              },
              "width": "${width}",
              "height": "HUG"
            },
            "children": [
              {
                "figmaNodeType": "TEXT",
                "properties": {
                  "text": "${label}",
                  "fontSize": "${labelSize}",
                  "fontFamily": "Inter", 
                  "fontWeight": "Medium",
                  "textColor": "${labelColor}"
                }
              },
              {
                "figmaNodeType": "FRAME",
                "properties": {
                  "autoLayout": {
                    "direction": "HORIZONTAL",
                    "padding": {
                      "left": "${inputPaddingX}",
                      "right": "${inputPaddingX}",
                      "top": "${inputPaddingY}",
                      "bottom": "${inputPaddingY}"
                    },
                    "spacing": "${inputSpacing}"
                  },
                  "cornerRadius": "${cornerRadius}",
                  "fills": [{
                    "color": "${inputBackground}"
                  }],
                  "strokes": [{
                    "color": "${borderColor}",
                    "width": "${borderWidth}"
                  }],
                  "width": "FILL",
                  "height": "HUG"
                },
                "children": [
                  {
                    "figmaNodeType": "TEXT",
                    "properties": {
                      "text": "${placeholder}",
                      "fontSize": "${inputSize}",
                      "fontFamily": "Inter",
                      "fontWeight": "Regular",
                      "textColor": "${placeholderColor}"
                    }
                  },
                  {
                    "figmaNodeType": "RECTANGLE",
                    "properties": {
                      "width": "${iconSize}",
                      "height": "${iconSize}",
                      "fills": [{
                        "color": "${iconColor}"
                      }],
                      "cornerRadius": 2
                    }
                  }
                ]
              },
              {
                "figmaNodeType": "TEXT",
                "properties": {
                  "text": "${helpText}",
                  "fontSize": "${helpSize}",
                  "fontFamily": "Inter",
                  "fontWeight": "Regular", 
                  "textColor": "${helpColor}"
                }
              }
            ],
            "behavior": {
              "createVariants": true,
              "variantGrouping": "single",
              "componentProperties": {
                "label": {
                  "type": "TEXT",
                  "defaultValue": "Label"
                },
                "placeholder": {
                  "type": "TEXT", 
                  "defaultValue": "Enter text..."
                },
                "helpText": {
                  "type": "TEXT",
                  "defaultValue": "Helper text"
                },
                "showIcon": {
                  "type": "BOOLEAN",
                  "defaultValue": false
                },
                "showHelp": {
                  "type": "BOOLEAN",
                  "defaultValue": true
                }
              }
            }
          },
          "properties": {
            "label": "Email address",
            "placeholder": "Enter your email",
