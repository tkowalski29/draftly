{
  "version": "1.0.0",
  "description": "Generic Component Renderer Examples - demonstracja elastyczno≈õci systemu",
  "pages": [
    {
      "name": "Generic Renderer Examples",
      "nodes": [
        {
          "name": "[Card] Product System",
          "type": "GENERIC",
          "componentConfig": {
            "figmaNodeType": "FRAME",
            "properties": {
              "autoLayout": {
                "direction": "VERTICAL",
                "padding": {
                  "left": "${padding}",
                  "right": "${padding}",
                  "top": "${padding}",
                  "bottom": "${padding}"
                },
                "spacing": "${spacing}"
              },
              "cornerRadius": "${cornerRadius}",
              "fills": [{
                "color": "${backgroundColor}",
                "opacity": "${opacity}"
              }],
              "strokes": [{
                "color": "${borderColor}",
                "width": "${borderWidth}"
              }],
              "width": "${width}",
              "height": "HUG"
            },
            "children": [
              {
                "figmaNodeType": "RECTANGLE",
                "properties": {
                  "width": "FILL",
                  "height": "${imageHeight}",
                  "fills": [{
                    "color": "${imageColor}"
                  }],
                  "cornerRadius": "${imageCornerRadius}"
                }
              },
              {
                "figmaNodeType": "FRAME",
                "properties": {
                  "autoLayout": {
                    "direction": "VERTICAL",
                    "spacing": "${contentSpacing}"
                  },
                  "width": "FILL",
                  "height": "HUG"
                },
                "children": [
                  {
                    "figmaNodeType": "TEXT",
                    "properties": {
                      "text": "${title}",
                      "fontSize": "${titleSize}",
                      "fontFamily": "Inter",
                      "fontWeight": "Bold",
                      "textColor": "${titleColor}"
                    }
                  },
                  {
                    "figmaNodeType": "TEXT",
                    "properties": {
                      "text": "${description}",
                      "fontSize": "${descriptionSize}",
                      "fontFamily": "Inter",
                      "fontWeight": "Regular",
                      "textColor": "${descriptionColor}"
                    }
                  },
                  {
                    "figmaNodeType": "FRAME",
                    "properties": {
                      "autoLayout": {
                        "direction": "HORIZONTAL",
                        "spacing": "${priceSpacing}",
                        "alignment": "CENTER"
                      },
                      "width": "FILL",
                      "height": "HUG"
                    },
                    "children": [
                      {
                        "figmaNodeType": "TEXT",
                        "properties": {
                          "text": "${price}",
                          "fontSize": "${priceSize}",
                          "fontFamily": "Inter",
                          "fontWeight": "Bold",
                          "textColor": "${priceColor}"
                        }
                      },
                      {
                        "figmaNodeType": "TEXT",
                        "properties": {
                          "text": "${originalPrice}",
                          "fontSize": "${originalPriceSize}",
                          "fontFamily": "Inter", 
                          "fontWeight": "Regular",
                          "textColor": "${originalPriceColor}",
                          "textDecoration": "STRIKETHROUGH"
                        }
                      }
                    ]
                  }
                ]
              }
            ],
            "behavior": {
              "createVariants": true,
              "variantGrouping": "single",
              "componentProperties": {
                "title": {
                  "type": "TEXT",
                  "defaultValue": "Product Name"
                },
                "description": {
                  "type": "TEXT",
                  "defaultValue": "Product description goes here"
                },
                "price": {
                  "type": "TEXT",
                  "defaultValue": "$29.99"
                },
                "showOriginalPrice": {
                  "type": "BOOLEAN",
                  "defaultValue": false
                }
              }
            }
          },
          "properties": {
            "title": "Wireless Headphones",
            "description": "Premium quality sound with noise cancellation",
            "price": "$199.99",
            "originalPrice": "$249.99",
            "backgroundColor": "#FFFFFF",
            "borderColor": "#E5E7EB",
            "borderWidth": 1,
            "titleColor": "#1F2937",
            "descriptionColor": "#6B7280",
            "priceColor": "#059669",
            "originalPriceColor": "#9CA3AF",
            "imageColor": "#F3F4F6",
            "width": 280,
            "imageHeight": 180,
            "padding": 16,
            "spacing": 12,
            "contentSpacing": 8,
            "priceSpacing": 8,
            "cornerRadius": 8,
            "imageCornerRadius": 6,
            "titleSize": 18,
            "descriptionSize": 14,
            "priceSize": 16,
            "originalPriceSize": 14,
            "opacity": 1
          },
          "variants": [
            {
              "name": "default",
              "getFromParent": true
            },
            {
              "name": "compact",
              "getFromParent": true,
              "width": 240,
              "imageHeight": 140,
              "padding": 12,
              "spacing": 8,
              "titleSize": 16,
              "descriptionSize": 13
            },
            {
              "name": "wide",
              "getFromParent": true,
              "width": 320,
              "imageHeight": 200,
              "padding": 20,
              "spacing": 16
            }
          ]
        },
        {
          "name": "[Badge] Status System",
          "type": "GENERIC",
          "componentConfig": {
            "figmaNodeType": "FRAME",
            "properties": {
              "autoLayout": {
