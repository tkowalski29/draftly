{
  "version": "1.0.0",
  "designSystem": {
    "pageName": "ðŸ¤– Design System",
    "atomicDesign": {
      "hierarchy": {
        "Atoms": {
          "level": 0,
          "types": ["GENERIC"],
          "files": ["generic-configs/atoms/icon-config.json"],
          "renderer": "ICON_INHERITANCE"
        },
        "Molecules": {
          "level": 1,
          "types": ["GENERIC"],
          "files": ["generic-configs/molecules/button-config.json"],
